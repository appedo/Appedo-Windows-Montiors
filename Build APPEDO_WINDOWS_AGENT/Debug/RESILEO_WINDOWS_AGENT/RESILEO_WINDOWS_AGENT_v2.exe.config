    <add key="port" value="443" />
    <add key="path" value="appedo_collector" />
    <add key="processNames" value="winlogbeat,filebeat" />
    <add key="enableTopProcessMemory" value="false" />
    <add key="SqlServer" value="DESKTOP-1UO47BG\SQLEXPRESS" />
